import bpy

#create box
bpy.ops.mesh.primitive_cube_add()
box=bpy.context.object
box.name='box'

#set box dimension
box.dimensions = (4.0,4.0,4.0)

#create lid
bpy.ops.mesh.primitive_cube_add()
lid=bpy.context.object
lid.name='lid'

#set lid dimension
lid.dimensions = (4.4,4.4,1.0)

#set lid location
lid.location = (0.0,0.0,2.4)
